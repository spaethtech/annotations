{
    "name": "spaethtech/annotations",
    "description": "A semi-complex PHP-Doc Annotation parsing library, specifically designed for use in my own projects!",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@spaethtech.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "spaethtech/common": "dev-master",
        "spaethtech/collections": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "consolidation/robo": "^3"
    },
    "autoload": {
        "psr-4": {
            "SpaethTech\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpaethTech\\": [
                "tests/",
                "examples/"
            ]
        }
    }
}
