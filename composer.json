{
    "name": "mvqn/annotations",
    "description": "A semi-complex PHP-Doc Annotation parsing library, specifically designed for use in MVQN projects, but generic enough for anything!",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",

        "ext-curl": "*",
        "ext-json": "*",

        "mvqn/common": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8"
    },
    "autoload": {
        "psr-4": {
            "MVQN\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MVQN\\": [ "tests/", "examples/" ]
        }
    }
}
