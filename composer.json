{
    "name": "rspaeth/annotations",
    "description": "A semi-complex PHP-Doc Annotation parsing library, specifically designed for use in my own projects!",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
    ],
    "require": {
        "php": "~7.3",

        "ext-curl": "*",
        "ext-json": "*",

        "rspaeth/common": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9"
    },
    "autoload": {
        "psr-4": {
            "rspaeth\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rspaeth\\": [ "./tests/", "./examples/" ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        { "type": "path", "url": "../*"}
    ]
}
